@charset "utf-8";
/* CSS Document */
/* reset */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, input, button, textarea, p, blockquote, th, td {
	margin:0;
	padding:0;
	font-family: "Microsoft Yahei","黑体","宋体";
}
img {
	border:none;
	vertical-align:top;
}/*无边框， 解决ie6下边间距bug */
ul {
	font-size:14px;
	text-align:left;
	margin:0;

}
li {
	list-style:none;
	margin-top:0px;
	font-size:14px;
}
a {
	text-decoration:none;
	color:#bf8729;
}
a {
	outline: none;
}
a:active {
star:expression(this.onFocus=this.blur());
}
/*====浮动和清除浮动=====*/
.left {
	float:left;
}
.right {
	float:right;
}
.clear {
	clear:both;
}
.clearfix:after {
content:”.”;
	display:block;
	height:0px;
	clear:both;
	visibility:hidden;
}
.clearfix {
	zoom:1;
}
/*栏目之间纵间距*/
.blank10 {
	clear:both;
	height:10px;
	overflow:hidden;
	display:block;
}
.blank15 {
	clear:both;
	height:15px;
	overflow:hidden;
	display:block;
}
.blank20 {
	clear:both;
	height:20px;
	overflow:hidden;
	display:block;
}
.blank60 {
	clear:both;
	height:60px;
	overflow:hidden;
	display:block;
}
/*通用_基本信息*/
.header{ background-image:url(../images/headbg.jpg); background-repeat:repeat; width:100%; height:120px; margin:0px; padding:0px;}
.content{ width:1000px; margin:0 auto;  height:120px;}
.logo{ background-image:url(../images/logo.png); float:left; height:100px; width:231px;}
.tel{ background-image:url(../images/tel.png); float:right; height:108px; width:216px;}
.swap{
	width:100%; height:400px; margin:0 auto;
}
#slider{
	list-style:none; width:100%; height:100%; margin:0; padding:0; 
}
#slider li{
	width:100%; height:491px; display:none; position:absolute;
}

#naviSlider{
	list-style:none;  margin:0 auto; width:auto; text-align:center; margin-top:43px; *margin-top:45px; z-index:999;
	position:relative;
	
}
#naviSlider  li{
	height:10px; width:10px;   cursor:pointer; display:inline-block; 
	background:url(../images/dot.png) -10px center no-repeat;
	_background:url(../images/dot_ie6.png) -10px center no-repeat;
	*zoom:1;*display:inline;*margin-left:5px;
	
}
#naviSlider li.on{
	background:url(../images/dot.png) left center no-repeat;
	_background:url(../images/dot_ie6.png) left center no-repeat;
}
.login{ width:100%; height:491px; margin:0 auto; position:absolute; z-index:9; }
.login_in{ width:1000px; height:491px; margin:0 auto;}
.login_bg {
	  Z-INDEX: 2; BACKGROUND: url(../images/login_bg.png) no-repeat;  MARGIN-LEFT: 151px; WIDTH: 315px;   HEIGHT: 336px; TEXT-ALIGN: left; float:right; margin-top:80px;box-shadow: 0px 1px 3px 0 rgba(0,0,0,0.2); webkit-box-shadow: 0px 1px 3px 0 rgba(0,0,0,0.2); moz-box-shadow: 0px 1px 3px 0 rgba(0,0,0,0.2)
}
.loginFuncNormal {
	FONT-SIZE: 24px; FLOAT: left; OVERFLOW: hidden; WIDTH: 147px; CURSOR: pointer; COLOR: #294e96; LINE-HEIGHT: 48px; POSITION: relative; HEIGHT: 48px; TEXT-ALIGN: center;margin-top:22px; margin-left:80px; display:inline; float:left;
}
.login_field2, .login_field1, .login_field {
	float:left;
	height: 36px;
	width:222px;
	margin-top:18px;
	display:inline;
	position:relative;
	background-image:url(../images/input_bg.png);
	background-repeat:no-repeat;
	margin-left:48px;
	line-height: 36px;
}
.login_ver{ line-height:36px; color:#C06; font-size:16px; float:left; display:inline; margin:15px 0px 0px 8px;}
.login_field1{	background-image:url(../images/input_bg1.png);
	background-repeat:no-repeat; width:94px;}
.login_field2{background-image:url(../images/input_btn.png);
	background-repeat:no-repeat; width:135px; height:40px; cursor:pointer;}
.login_field2 input, .login_field1 input,.login_field input {
	position:relative;
	width: 222px;
	height: 36px;
	margin:0px 5px 0px 5px;
	background:transparent;
	color: #585858;
	line-height: 36px;
	border:none;
	display:inline;
	
}
.login_field1 input{ width:94px;}
.login_field2 input{ width:135px;  height:40px; margin:0px; padding:0px; font-size:18px; color:#fff;cursor:pointer; }


.footer{text-align:center;clear:both; margin-top:50px; padding-top:20px;}
.footer p{ color:#666666; height:28px; font-size:14px;}
.footer a{ color:#666666;}
.footer a:hover{ color:#1e72ca;}