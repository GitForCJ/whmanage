package com.wlt.webm.gm.form;
/**
 * 
* @ClassName: GuanMingForm 
* @Description: 冠铭游戏API接口 
* @author tanwanlong
* @date 2014-6-5 下午02:37:38
 */
public class GameInterface {
	private String ret;//状态编码 0 为查询成功
	private String ret_msg;//错误提示信息
	private String gameapi;//游戏接口编码
	private String gamename;//游戏接口名称
	private String lei;//所属游戏厂商名称
	private String lx;//接口类型 1为官方直充
	private String zt;//接口状态 1为开通; 0为维护
	private String mprice;//市场价格 面值
	private String lprice;//默认折扣
	private String startbuy;//控制数量购买起购量
	private String endbuy;//控制数量购买截止量
	private String contbuy;//购买控制数量 如：2,3,6,8,10 逗号相隔
	private String buyunit;//数量单位，如个，张
	private String gtext;//说明
	
	
	public GameInterface(String buyunit, String contbuy, String endbuy, String gameapi,
			String gamename, String gtext, String lei, String lprice,
			String lx, String mprice, String ret, String ret_msg,
			String startbuy, String zt) {
		super();
		this.buyunit = buyunit;
		this.contbuy = contbuy;
		this.endbuy = endbuy;
		this.gameapi = gameapi;
		this.gamename = gamename;
		this.gtext = gtext;
		this.lei = lei;
		this.lprice = lprice;
		this.lx = lx;
		this.mprice = mprice;
		this.ret = ret;
		this.ret_msg = ret_msg;
		this.startbuy = startbuy;
		this.zt = zt;

	}
	public GameInterface() {
		super();
		// TODO Auto-generated constructor stub
	}
	public String getRet() {
		return ret;
	}

	public void setRet(String ret) {
		this.ret = ret;
	}

	public String getRet_msg() {
		return ret_msg;
	}

	public void setRet_msg(String ret_msg) {
		this.ret_msg = ret_msg;
	}
	public String getGameapi() {
		return gameapi;
	}
	public void setGameapi(String gameapi) {
		this.gameapi = gameapi;
	}
	public String getGamename() {
		return gamename;
	}
	public void setGamename(String gamename) {
		this.gamename = gamename;
	}
	public String getLei() {
		return lei;
	}
	public void setLei(String lei) {
		this.lei = lei;
	}
	public String getLx() {
		return lx;
	}
	public void setLx(String lx) {
		this.lx = lx;
	}
	public String getZt() {
		return zt;
	}
	public void setZt(String zt) {
		this.zt = zt;
	}
	public String getMprice() {
		return mprice;
	}
	public void setMprice(String mprice) {
		this.mprice = mprice;
	}
	public String getLprice() {
		return lprice;
	}
	public void setLprice(String lprice) {
		this.lprice = lprice;
	}
	public String getStartbuy() {
		return startbuy;
	}
	public void setStartbuy(String startbuy) {
		this.startbuy = startbuy;
	}
	public String getEndbuy() {
		return endbuy;
	}
	public void setEndbuy(String endbuy) {
		this.endbuy = endbuy;
	}
	public String getContbuy() {
		return contbuy;
	}
	public void setContbuy(String contbuy) {
		this.contbuy = contbuy;
	}
	public String getBuyunit() {
		return buyunit;
	}
	public void setBuyunit(String buyunit) {
		this.buyunit = buyunit;
	}
	public String getGtext() {
		return gtext;
	}
	public void setGtext(String gtext) {
		this.gtext = gtext;
	}
	

}
