package com.wlt.webm.gm.form;

import java.util.List;

public class GameArea {
	private String id ;
	private String name;
	private List<GameServer> litGameServer;
	public GameArea() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public GameArea(String id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public List<GameServer> getLitGameServer() {
		return litGameServer;
	}

	public void setLitGameServer(List<GameServer> litGameServer) {
		this.litGameServer = litGameServer;
	}
	
}
