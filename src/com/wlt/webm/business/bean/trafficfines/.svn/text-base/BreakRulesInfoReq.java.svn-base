package com.wlt.webm.business.bean.trafficfines;

/**
 * 违章代办详单查询请求对象
 * 
 * @author 1989
 * 
 */
public class BreakRulesInfoReq {
	/**
	 * 车牌
	 */
	private String plateNumber;
	/**
	 * 手机号码
	 */
	private String contactNum;
	/**
	 * 页数
	 */
	private int page;
	/**
	 * 条数
	 */
	private int pageSize;
	/**
	 * 用于安全性验证
	 */
	private String tokenId;
	
	/**
	 * 构造函数
	 * @param plateNumber  车牌
	 * @param contactNum   手机号码
	 * @param page  页数
	 * @param pageSize  条数
	 * @param tokenId  安全验证tokenid
	 */
	public BreakRulesInfoReq(String plateNumber,String contactNum,
			int page,int pageSize,String tokenId){
		this.plateNumber=plateNumber;
		this.contactNum=contactNum;
		this.page=page;
		this.pageSize=pageSize;
		this.tokenId=tokenId;
	}
	public String getPlateNumber() {
		return plateNumber;
	}
	public void setPlateNumber(String plateNumber) {
		this.plateNumber = plateNumber;
	}
	public String getContactNum() {
		return contactNum;
	}
	public void setContactNum(String contactNum) {
		this.contactNum = contactNum;
	}
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	public int getPageSize() {
		return pageSize;
	}
	public void setPageSize(int pageSize) {
		this.pageSize = pageSize;
	}
	public String getTokenId() {
		return tokenId;
	}
	public void setTokenId(String tokenId) {
		this.tokenId = tokenId;
	}
}
