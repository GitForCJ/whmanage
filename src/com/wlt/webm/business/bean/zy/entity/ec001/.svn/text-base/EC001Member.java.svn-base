package com.wlt.webm.business.bean.zy.entity.ec001;

import java.util.List;
/**
 * 
 * @author fw
 *成员列表信息
 */
public class EC001Member {
	private String OptType;
	 private String PayFlag;
	 private String UsecyCle;
	 private String Mobile;
	 private String UserName;
	 private String EffType;
	 private List<EC001PrdList> PrdList;//成员产品列表
	public String getOptType() {
		return OptType;
	}
	/**
	 * 操作类型 0-开通 1-暂停 2-注销 3-恢复 4-修改
	 * @param optType
	 */
	public void setOptType(String optType) {
		OptType = optType;
	}
	public String getPayFlag() {
		return PayFlag;
	}
	/**
	 *计费标志 0＝集团代付，1＝个人支付
	 * @param payFlag
	 */
	public void setPayFlag(String payFlag) {
		PayFlag = payFlag;
	}
	public String getUsecyCle() {
		return UsecyCle;
	}
	/**
	 * 使用周期数 使用的月数,8585和8584产品必填
	 * @param usecyCle
	 */
	public void setUsecyCle(String usecyCle) {
		UsecyCle = usecyCle;
	}
	public String getMobile() {
		return Mobile;
	}
	public void setMobile(String mobile) {
		Mobile = mobile;
	}
	public String getUserName() {
		return UserName;
	}
	public void setUserName(String userName) {
		UserName = userName;
	}
	public String getEffType() {
		return EffType;
	}
	/**
	 * 生效方式 取值：立即(2),下月(3),默认(0) 
	 * @param effType
	 */
	public void setEffType(String effType) {
		EffType = effType;
	}
	public List<EC001PrdList> getPrdList() {
		return PrdList;
	}
	public void setPrdList(List<EC001PrdList> prdList) {
		PrdList = prdList;
	}
	 
}
