package oufei;
public class OuFeiOrderInfo {

	private String order_id ;  //订单号
	private String id ;   //
	private String product_par_value ; //充值金额
	private String recharge_account ;  //充值号码
	public OuFeiOrderInfo(){}
	//构造函数
	public OuFeiOrderInfo(String order_id, String id,  String product_par_value,
			String recharge_account) {
		super();
		this.order_id = order_id;
		this.id = id;
		this.product_par_value = product_par_value;
		this.recharge_account = recharge_account;
	}
	//set and get method 
	public String getOrder_id() {
		return order_id;
	}
	public void setOrder_id(String order_id) {
		this.order_id = order_id;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getProduct_par_value() {
		return product_par_value;
	}
	public void setProduct_par_value(String product_par_value) {
		this.product_par_value = product_par_value;
	}
	public String getRecharge_account() {
		return recharge_account;
	}
	public void setRecharge_account(String recharge_account) {
		this.recharge_account = recharge_account;
	}
}

