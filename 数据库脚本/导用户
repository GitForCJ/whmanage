SELECT a.`user_login`,a.`user_ename`,b.`sa_name`,a.`user_adress`
 ,a.`user_tel` FROM sys_user a ,sys_area b WHERE a.user_city=b.sa_id AND
a.`user_role` IN(52,51,61,42,66,63,69,70)

SELECT * FROM sys_area

SELECT * FROM sys_user

