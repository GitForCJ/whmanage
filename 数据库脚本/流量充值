SELECT * FROM sys_interface

SELECT * FROM sys_interface2face WHERE interfaceID=35

SELECT * FROM wht_flowprice WHERE TYPE=1

SELECT * FROM wht_flowproduct

SELECT * FROM sys_tpemploy_Flow WHERE TYPE=1


SELECT fl.name,fl.price,(SELECT tp.value FROM sys_tpemploy_Flow tp WHERE tp.type='1' 
AND tp.cm_addr=0 AND tp.groups='30001' LIMIT 0,1) AS yj FROM wht_flowprice fl  
WHERE fl.type='1' AND fl.name IN(aa.substring(0,aa.length()-1))

SELECT * FROM sys_flowinterfaceMaping WHERE TYPE='1' AND pid='35'

SELECT flow.flow_interId,flow.exp1 FROM sys_Flow flow,(SELECT map.interid FROM sys_flowinterfaceMaping map WHERE 
map.type='1' AND pid='35' LIMIT 0,1) AS ac WHERE flow.operator_type='1' AND flow.flow_interId=ac.interid



